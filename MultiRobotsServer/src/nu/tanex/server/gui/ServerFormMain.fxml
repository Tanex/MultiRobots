                              <Insets left="50.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="discardButton" mnemonicParsing="false" text="Discard" />
                     </children>
                     <padding>
                        <Insets top="25.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
            </VBox>
        </items>
      </SplitPane>
   </children>
</Pane>
